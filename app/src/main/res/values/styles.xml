<resources>

    <style name="ItemLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">84dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="BtnGreen">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">46dp</item>
        <item name="android:layout_marginLeft">38dp</item>
        <item name="android:layout_marginRight">38dp</item>
        <item name="android:background">@drawable/btn_green</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/font_18</item>
    </style>

    <style name="LineAccountGray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#D8D8D8</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="LineGrayE8">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#E8E8E8</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="AccountGreenText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#37C536</item>
        <item name="android:textSize">@dimen/font_12</item>
    </style>

    <style name="AccountEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/font_41</item>
        <item name="android:textColorHint">@color/font_B3</item>
        <item name="android:textSize">@dimen/font_16</item>
    </style>

    <style name="TabText">
        <item name="android:textSize">@dimen/font_17</item>
        <item name="android:textColor">@color/font_41</item>
    </style>

    <style name="TabTextSeedStore">
        <item name="android:textSize">@dimen/font_17</item>
        <item name="android:textColor">@color/font_36</item>
    </style>

    <style name="SelectableStyle">
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
    </style>

    <style name="HomeBottomModule">
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/font_11</item>
    </style>

</resources>